# {{ ansible_managed }}

# Note this is used directly by the scheduler but not
# by the workers.  Worker environment is set in supervisor
# config.
{% do xserver_env_config.update(XSERVER_ENV_EXTRA) %}
{% for name,value in xserver_env_config.items() %}
{% if value %}
export {{ name }}="{{ value }}"
{% endif %}
{% endfor %}

export DJANGO_SETTINGS_MODULE=xserver.settings_local
